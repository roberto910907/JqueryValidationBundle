{
    "name": "roberto910907/jquery-validation-bundle",
    "type": "symfony-bundle",
    "keywords": [ "jquery", "form", "validation", "validator", "symfony", "twig"],
    "description": "Jquery form validation bundle for symfony 4",
    "require": {
        "php": "^7.2.5",
        "symfony/framework-bundle": "4.4.*",
        "symfony/dependency-injection": "4.4.*|dev-master",
        "symfony/form": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/twig-pack": "^1.0",
        "symfony/twig-bridge":" ~2.3|~3.0|4.4.*",
        "doctrine/collections": "~1.0"
    },
    "require-dev": {
        "symfony/browser-kit": "4.4.*",
        "symfony/dom-crawler": "4.4.*",
        "symfony/finder": "~2.3|~3.0|4.4.*",
        "symfony/yaml": "~2.3|~3.0|4.4.*",
        "symfony/css-selector": "~2.3|~3.0",

        "phpunit/phpunit": "~4.7@stable",
        "symfony/phpunit-bridge": "~2.7@stable",

        "behat/behat": "~3.0",
        "behat/mink": "~1.6",
        "behat/mink-selenium2-driver":  "~1.1",
        "behat/mink-extension": "~2.0",
        "behat/symfony2-extension": "~2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Roberto Rielo",
            "email": "roberto910907@gmail.com"
        },
        {
            "name": "Juan Miguel Leal",
            "email": "jmleal1990@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Boekkooi\\Bundle\\JqueryValidationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Boekkooi\\Bundle\\JqueryValidationBundle\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
